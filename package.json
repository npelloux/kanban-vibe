{
  "name": "kanban-vibe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:report": "vitest run && npx allure-commandline generate allure-results -o allure-report --clean",
    "test:serve": "npx allure-commandline serve allure-results",
    "report:open": "npx allure-commandline open allure-report",
    "preview": "vite preview",
    "deploy": "npm install --global vercel@latest && vercel pull --yes --environment=preview --token=${{ secrets.VERCEL_TOKEN }} && vercel deploy --prebuilt"
  },
  "dependencies": {
    "chart.js": "^4.4.9",
    "jq": "^1.7.2",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/runner": "^4.0.17",
    "allure-vitest": "^3.4.4",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  }
}
